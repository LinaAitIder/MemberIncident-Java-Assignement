<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="application.controller.MainController">
   <top>
      <VBox alignment="TOP_CENTER" prefHeight="305.0" prefWidth="361.0" BorderPane.alignment="CENTER">
         <children>
            <Label prefHeight="37.0" prefWidth="256.0" text="Ajouter un nouveau member">
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <font>
                  <Font size="14.0" />
               </font>
            </Label>
            <TextField fx:id="nom" promptText="Nom">
               <VBox.margin>
                  <Insets bottom="10.0" left="40.0" right="40.0" top="40.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="prenom" promptText="Prenom">
               <VBox.margin>
                  <Insets bottom="10.0" left="40.0" right="40.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="email" promptText="Email">
               <VBox.margin>
                  <Insets bottom="10.0" left="40.0" right="40.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <TextField fx:id="phone" promptText="Phone">
               <VBox.margin>
                  <Insets bottom="10.0" left="40.0" right="40.0" top="10.0" />
               </VBox.margin>
            </TextField>
            <Button fx:id="insertBtn" mnemonicParsing="false" onAction="#InsertMember" text="Inserer" />
         </children>
      </VBox>
   </top>
</BorderPane>
